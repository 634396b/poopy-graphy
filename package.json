{
  "name": "poopy-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn lint:check",
    "lint:write": "prettier --write .",
    "lint:check": "prettier --check .",
    "types:check": "tsc",
    "gql:bitquery:run:schema": "ts-node -r tsconfig-paths/register -r ./scripts/useLocalConfig.js ./scripts/bitquery/run.schema.ts",
    "gql:bitquery:run:queries": "ts-node -r tsconfig-paths/register -r ./scripts/useLocalConfig.js ./scripts/bitquery/run.queries.ts",
    "gql:codegen": "graphql-codegen",
    "gql:all": "yarn run gql:bitquery:run:schema && yarn run gql:codegen",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "date-fns": "^2.21.1",
    "dotenv": "^9.0.2",
    "graphql": "^15.5.0",
    "ioredis": "^4.27.2",
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-use": "^17.2.4",
    "ts-node": "^9.1.1",
    "web3": "^1.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/core": "^1.17.9",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-document-nodes": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@tsconfig/recommended": "^1.0.1",
    "@types/ioredis": "^4.26.3",
    "@types/node": "^15.0.1",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.4",
    "babel-loader": "^8.2.2",
    "graphql-tools": "^7.0.4",
    "husky": "^6.0.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
